{
  "compilerOptions": {
    "target": "es2015",
    "module": "commonjs",
    "rootDir": "./src",
     "moduleResolution": "node",
     "resolveJsonModule": true,
    "declaration": true,
    "outDir": "./build",
     "removeComments": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
     "noImplicitAny": true,
     "useUnknownInCatchVariables": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  }
}